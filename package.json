{
    "scripts": {
        "start": "node -r tsconfig-paths/register dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "clean": "rimraf dist",
        "build:prisma": "prisma generate",
        "build:ts": "tsc",
        "build": "npm-run-all clean build:prisma build:ts",
        "lint": "eslint src/**/*.ts",
        "test": "jest",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
    },
    "dependencies": {
        "@godaddy/terminus": "^4.12.1",
        "@prisma/client": "^6.15.0",
        "@types/node": "^24.3.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^2.2.0",
        "compression": "^1.8.1",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "module-alias": "^2.2.3",
        "mongoose": "^8.18.1",
        "morgan": "^1.10.1",
        "pg": "^8.16.3",
        "prisma": "^6.16.1",
        "redis": "^5.8.2",
        "socket.io": "^4.8.1",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.8.1",
        "@types/express": "^5.0.3",
        "@types/module-alias": "^2.0.4",
        "@types/morgan": "^1.9.10",
        "@types/redis": "^4.0.11",
        "nodemon": "^3.1.10",
        "npm-run-all": "^4.1.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.2"
    }
